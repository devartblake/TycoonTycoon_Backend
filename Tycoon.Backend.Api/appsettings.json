{
  "ConnectionStrings": {
    "db": "Host=localhost;Port=5432;Database=tycoon;Username=postgres;Password=postgres",
    "redis": "localhost:6379,password=change-me,defaultDatabase=0",
    "mongo": "mongodb://localhost:27017/tycoon_db?authSource=tycoon_db",
    "elasticsearch": "http://elastic:elastic@localhost:9200"
  },
  "Mongo": {
    "Database": "tycoon_analytics"
  },
  "Elastic": {
    "Url": "http://localhost:9200",
    "DailyWriteAlias": "tycoon-qa-daily-rollups",
    "PlayerDailyWriteAlias": "tycoon-qa-player-daily-rollups"
  },
  "AdminOps": {
    "Enabled": true,
    "Header": "X-Admin-Ops-Key",
    "Key": "CHANGE_ME"
  },
  "Observability": {
    "ServiceName": "Tycoon.Backend.Api",
    "OtlpEndpoint": ""
  },
  "Serilog": {
    "MinimumLevel": "Information"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Aspire.Hosting.Dcp": "Warning"
    }
  },
  "Parameters": {
    "admin": "admin",
    "postgres-admin": "postgres",
    "admin-password": "admin"
  }
}